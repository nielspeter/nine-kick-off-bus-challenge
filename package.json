    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:reset": "docker-compose -f docker-compose.dev.yml down -v && docker-compose -f docker-compose.dev.yml up -d",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f app-dev",
    "db:seed": "node scripts/seed-database.js",
    "db:seed-api": "curl -X POST http://localhost:3000/api/seed",
    "fetch:nine-employees": "node scripts/generate-seed-sql.js",
    "generate:seed-file": "node scripts/generate-seed-sql.js > database/seed.sql",
    "update:docker-seed": "mkdir -p init-db && node scripts/generate-seed-sql.js > init-db/001-seed.sql",
    "start:with-db": "scripts/start-with-db.sh",
    "dev:setup": "./scripts/dev-up.sh",
    "dev:teardown": "./scripts/dev-down.sh"
  },
  "dependencies": {
